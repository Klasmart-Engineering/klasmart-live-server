{
  "name": "kidsloop-live-server",
  "version": "1.8.1",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "ts-node-dev src/main.ts",
    "lint": "eslint --fix .",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:compile && npm run build:copy-assets",
    "build:compile": "tsc -p tsconfig.json --outDir ./dist",
    "build:copy-assets": "cp README.md ./dist && cp package.json ./dist",
    "release": "standard-version --no-verify",
    "coverage": "nyc -r lcov -e .ts -x \"*[Tt]est.ts\" npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/calmisland/kidsloop-live-server.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/calmisland/kidsloop-live-server#readme",
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@graphql-tools/schema": "^8.3.1",
    "@kidsloop/eslint-config": "bitbucket:calmisland/kidsloop-eslint-config",
    "@newrelic/apollo-server-plugin": "^1.1.1",
    "@types/cookie": "^0.4.1",
    "@types/ioredis": "^4.28.1",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/newrelic": "^7.0.2",
    "@types/uuid": "^7.0.4",
    "@types/ws": "^7.2.5",
    "cz-conventional-changelog": "^3.0.1",
    "husky": "^7.0.4",
    "nyc": "^15.1.0",
    "standard-version": "^9.3.2",
    "ts-loader": "^9.2.6",
    "ts-mockito": "^2.6.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "apollo-server-express": "^3.5.0",
    "aws-sdk": "^2.926.0",
    "axios": "^0.21.4",
    "cookie": "^0.4.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^16.0.1",
    "graphql-request": "^3.5.0",
    "graphql-scalars": "^1.8.0",
    "ioredis": "^4.28.0",
    "jsonwebtoken": "^8.5.1",
    "kidsloop-token-validation": "bitbucket:calmisland/kidsloop-token-validation",
    "newrelic": "^8.4.0",
    "subscriptions-transport-ws": "^0.11.0",
    "typeorm": "^0.2.29",
    "uuid": "^8.1.0",
    "ws": "^7.4.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
