{
  "name": "kidsloop-live-server",
  "version": "1.14.5",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "ts-node-dev --debug src/main.ts",
    "lint": "eslint --fix '**/*.{ts,tsx}'",
    "clean": "rm -rf ./dist",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --runInBand  --forceExit -c ./tests/unit/jest.unit.config.js",
    "test:integration": "jest --runInBand  --forceExit -c ./tests/integration/jest.integration.config.js",
    "build": "npm run clean && npm run build:compile && npm run build:copy-assets",
    "build:compile": "tsc -p tsconfig.json --outDir ./dist",
    "build:copy-assets": "cp README.md ./dist && cp package.json ./dist",
    "release": "standard-version --no-verify",
    "coverage": "nyc -r lcov -e .ts -x \"*[Tt]est.ts\" npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/calmisland/kidsloop-live-server.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/calmisland/kidsloop-live-server#readme",
  "devDependencies": {
    "@apollo/client": "^3.5.10",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@graphql-tools/schema": "^8.3.3",
    "@types/cookie": "^0.4.1",
    "@types/express-rate-limit": "^6.0.0",
    "@types/graphql-depth-limit": "^1.1.3",
    "@types/jest": "^27.4.1",
    "@types/jest-dev-server": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/newrelic": "^7.0.2",
    "@types/uuid": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.13.0",
    "cz-conventional-changelog": "^3.0.1",
    "eslint": "^8.13.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-security": "^1.5.0",
    "graphql-tools": "^8.2.5",
    "husky": "^7.0.4",
    "jest-dev-server": "^6.0.2",
    "nyc": "^15.1.0",
    "react": "^17.0.2",
    "standard-version": "^9.3.2",
    "ts-loader": "^9.2.6",
    "ts-mockito": "^2.6.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@kl-engineering/kidsloop-token-validation": "^1.6.0",
    "@newrelic/apollo-server-plugin": "^1.2.0",
    "@types/ws": "^8.5.3",
    "@types/yieldable-json": "^2.0.0",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "aws-sdk": "^2.926.0",
    "axios": "^0.21.4",
    "cookie": "^0.4.1",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "graphql": "^16.0.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-request": "^4.2.0",
    "graphql-scalars": "^1.17.0",
    "graphql-ws": "^5.8.1",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.4",
    "jest": "^28.1.0",
    "jsonwebtoken": "^9.0.0",
    "newrelic": "^8.4.0",
    "prom-client": "^14.0.1",
    "subscriptions-transport-ws": "^0.11.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
